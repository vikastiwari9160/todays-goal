{
    "name": "Today's Goal",
    "version": "0.0.1",
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' https://example.com; object-src 'self'",
    "content_scripts":[{
        "js":["popup.js"],
        "matches": ["http://*/*", "https://*/*"]
    }],
    "browser_action":{
        "default_popup": "popup.html",
        "default_icon": "logo.png"
    },
    "icons":{
        "128" : "logo.png"
    },
    "permissions":["activeTab"]
}